                GROUP BY netcard.asset_id HAVING count(netcard.asset_id) &lt;=
                (select count(id) FROM asset_link_relation relation where  relation.asset_id=netcard.asset_id or relation.parent_asset_id=netcard.asset_id and relation.status=1)
