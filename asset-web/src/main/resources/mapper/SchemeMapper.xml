        content,
        file_info,
        memo
        from
        <include refid="Table_Name"></include>
        <trim prefix="WHERE" prefixOverrides="AND|OR">
            <if test=" assetId != null">
                and asset_id = #{assetId}
            </if>
            <if test=" type != null">
                and type = #{type}
            </if>
            and status = 1
            order by id desc limit 1
        </trim>
    </select>

    <select id="findMemoById" resultMap="BaseResultMap" parameterType="com.antiy.common.base.ObjectQuery">
