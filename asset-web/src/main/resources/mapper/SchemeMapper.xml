    <!--通过资产ID和状态查询方案信息-->
    <select id="findSchemeByAssetIdAndGmtCreateTime" resultMap="BaseResultMap" parameterType="map">
        select
        sch.file_info,
        sch.memo
        from
        <include refid="Table_Name"></include>
        sch
        left join asset
        on asset.id = sch.asset_id
        left join asset_operation_record aor
        on aor.scheme_id = sch.id
        where asset_id = #{assetId} and aor.gmt_create = #{gmtCreateTime}
    </select>

    <!--通过资产ID查询方案信息-->
    <select id="findSchemeByAssetIdAndType" resultMap="BaseResultMap"
