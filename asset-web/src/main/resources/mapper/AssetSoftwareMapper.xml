        a.id,
        a.name,
        a.version,
        a.manufacturer,
        a.software_status as softwareStatus,
        a.release_time as releaseTime,
        b.software_id,
        count( b.software_id ) as assetCount
        from
        asset_software a
        left join asset_software_relation b on a.id = b.software_id and b.status = 1
        where 1 = 1
        <if test=" multipleQuery != null">
            and a.name LIKE CONCAT('%',#{multipleQuery},'%')
        </if>
        <if test=" categoryModel != null">
            and category_model = #{categoryModel }
        </if>
        <if test=" softwareStatus != null">
