    <select id="findListAssetSoftware" resultMap="BaseResultMap"
            parameterType="com.antiy.asset.vo.query.AssetSoftwareQuery">
        select
        asset_software.id,
        asset_software.name,
        asset_software.authorization,
        asset_software.operation_system,
        asset_software.manufacturer,
        asset_software.version,
        asset_software.software_status,
        asset_software.release_time,
        asset_software.gmt_create,
        asset_software.category_model ,
        asset_software.memo,
        asset_software.language,
        asset_software.path,
        asset_software.serial,
        asset_software.protocol
        <if test="(categoryModel != null and categoryModel != '') or (categoryModels != null and categoryModels.length > 0)">
            ,
            b.name as category_model_name
        </if>
        from
        <include refid="Table_Name"></include>
        <if test="(categoryModel != null and categoryModel != '') or (categoryModels != null and categoryModels.length > 0)">
            left join asset_category_model b on asset_software.category_model = b.id
        </if>

        <where>
            <trim prefixOverrides="and|or">
                <choose>
                    <when test="taskBussinessIds!= null and taskBussinessIds.size() > 0 and ids == null and id == null">
                        and (asset_software.status = 1 or asset_software.id in
                        <foreach index="index" item="item" collection="taskBussinessIds" open="(" separator=","
                                 close=")">
                            #{item}
                        </foreach>
                        )
                    </when>
                    <otherwise>
                        and asset_software.status = 1
                    </otherwise>
                </choose>
                <if test="(categoryModel != null and categoryModel != '') or (categoryModels != null and categoryModels.length > 0)">
                    and b.status=1
                </if>
                <include refid="Multiple_Query"></include>
                <if test=" multipleQuery != null">
                    and (
                    asset_software.name LIKE CONCAT('%',#{multipleQuery},'%')
                    or asset_software.serial LIKE CONCAT('%',#{multipleQuery},'%')
                    or asset_software.id LIKE CONCAT('%',#{multipleQuery},'%')
                    )
                </if>
            </trim>
        </where>
        order by
        <if test="ids!= null and ids.size() > 0">
            FIELD(`asset_software.id`,
            <foreach index="index" item="item" collection="ids" separator=",">
                #{item}
            </foreach>
            ) desc,
        </if>
        asset_software.id desc
        <if test="pageSize !=-1">
            LIMIT #{pageSize} offset #{pageOffset}
        </if>
    </select>
    <select id="checkRepeatSoftware" resultType="int" parameterType="com.antiy.asset.entity.AssetSoftware">
