    <select id="findCount" resultType="int" parameterType="com.antiy.asset.vo.query.AssetSoftwareQuery">
        select count(1) count from
        <include refid="Table_Name"></include>
        <if test="(categoryModel != null and categoryModel != '') or (categoryModels != null and categoryModels.length > 0)">
            left join asset_category_model b on asset_software.category_model = b.id
        </if>
        <where>
            <trim prefixOverrides="and|or">
                <choose>
                    <when test="taskBussinessIds!= null and taskBussinessIds.size() > 0 and ids == null and id == null">
                        and (asset_software.status = 1 or asset_software.id in
                        <foreach index="index" item="item" collection="taskBussinessIds" open="(" separator=","
                                 close=")">
                            #{item}
                        </foreach>
                        )
                    </when>
                    <otherwise>
                        and asset_software.status = 1
                    </otherwise>
                </choose>

                <if test="(categoryModel != null and categoryModel != '') or (categoryModels != null and categoryModels.length > 0)">
                    and b.status = 1
                </if>
                <include refid="Multiple_Query"></include>
                <if test=" multipleQuery != null">
                    and (
                    asset_software.name LIKE CONCAT('%',#{multipleQuery},'%')
                    or asset_software.serial LIKE CONCAT('%',#{multipleQuery},'%')
                    or asset_software.id LIKE CONCAT('%',#{multipleQuery},'%'))
                </if>
            </trim>
        </where>

    </select>
