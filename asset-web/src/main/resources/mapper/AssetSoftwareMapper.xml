            <foreach collection="softwareStatusList" item="item" separator="," open="(" close=")">
                #{item}
            </foreach>
        </if>
        <if test=" authorization != null">
            and authorization = #{authorization }
        </if>
        <if test=" reportSource != null">
            and report_source = #{reportSource }
        </if>
        <if test=" beginTime != null">
            and release_time &gt;= #{beginTime }
        </if>
        <if test=" endTime != null">
            and release_time &lt;= #{endTime }
        </if>
        and b.id is not null
        and a.status = 1
        group by
        a.id
    </select>
    <select id="findAssetInstallCount" parameterType="com.antiy.asset.vo.query.AssetSoftwareQuery"
            resultType="java.lang.Integer">
        select
        count(1) as count
        from
        asset_software_relation a
        left join asset_network_card b on a.id = b.asset_id and b.status = 1
        left join asset c on c.id = a.asset_id and c.status = 1
        left join asset_user d on c.responsible_user_id = d.id and d.status = 1
        where 1 = 1
        and a.software_id = #{id}
        <if test=" installStatus != null">
            and a.install_status = #{installStatus}
        </if>
        <if test=" multipleQuery != null">
            and (c.name LIKE CONCAT('%',#{multipleQuery},'%')
            or c.number LIKE CONCAT('%',#{multipleQuery},'%')
            or c.serial LIKE CONCAT('%',#{multipleQuery},'%'))
        </if>
        <if test=" installType != null">
            and a.install_type = #{installType}
        </if>
        <if test=" assetId != null">
            and a.asset_id = #{assetId}
        </if>
        and c.status = 1
    </select>
    <select id="findAssetInstallList" parameterType="com.antiy.asset.vo.query.AssetSoftwareQuery"
            resultType="com.antiy.asset.entity.AssetSoftwareInstall">
        select
        a.id,
        a.asset_id as assetId,
        a.software_id as softwareId,
        c.number,
        c.name,
        d.id as userId,
        d.name as userName,
        case
        when group_concat(b.ip_address) is not null then
        group_concat(b.ip_address)
        when (select concat(inner_ip,outer_ip) from asset_network_equipment where asset_id = a.id and status = 1) is not null then
        (select concat(inner_ip,outer_ip) from asset_network_equipment where asset_id = a.id and status = 1)
        when (select ip from asset_safety_equipment where asset_id = a.id and status = 1) is not null then
        (select ip from asset_safety_equipment where asset_id = a.id and status = 1)
        else '' end as ip,
        case
        when group_concat( b.mac_address ) is not null then
        group_concat( b.mac_address )
        when (select mac_address from asset_network_equipment where asset_id = a.id and status = 1) is not null then
        (select mac_address from asset_network_equipment where asset_id = a.id and status = 1)
        when (select mac from asset_safety_equipment where asset_id = a.id and status = 1) is not null then
        (select mac from asset_safety_equipment where asset_id = a.id and status = 1)
        else '' end as mac,
        c.serial,
        c.category_model as categoryModel,
        (select name from asset_category_model where id = c.category_model) as categoryModelName,
        c.manufacturer,
        a.install_time as installTime,
        a.install_status as installStatus,
        case
        when a.install_type is not null then a.install_type
        else c.install_type
        end as installType
        from
        asset_software_relation a
        left join asset_network_card b on a.id = b.asset_id and b.status = 1
        left join asset c on c.id = a.asset_id and c.status = 1 and c.id is not null
        left join asset_user d on c.responsible_user_id = d.id
        where 1 = 1

        and a.software_id = #{id}
        <if test=" installStatus != null">
            and a.install_status = #{installStatus}
        </if>
        <if test=" multipleQuery != null">
            and (name LIKE CONCAT('%',replace(replace(#{multipleQuery},'%','\%'),'_','\_'),'%')
            or number LIKE CONCAT('%',replace(replace(#{multipleQuery},'%','\%'),'_','\_'),'%')
            or serial LIKE CONCAT('%',replace(replace(#{multipleQuery},'%','\%'),'_','\_'),'%')
            or exists (select 1 from asset_network_card b where asset.id = b.asset_id and ip_address like
            CONCAT('%',replace(replace(#{multipleQuery},'%','\%'),'_','\_'),'%') and b.status = 1)
            or exists (select 1 from asset_network_equipment b where asset.id = b.asset_id and inner_ip like
            CONCAT('%',replace(replace(#{multipleQuery},'%','\%'),'_','\_'),'%') and b.status = 1)
            or exists (select 1 from asset_network_equipment b where asset.id = b.asset_id and outer_ip like
            CONCAT('%',replace(replace(#{multipleQuery},'%','\%'),'_','\_'),'%') and b.status = 1)
            or exists (select 1 from asset_safety_equipment b where asset.id = b.asset_id and ip like
            CONCAT('%',replace(replace(#{multipleQuery},'%','\%'),'_','\_'),'%') and b.status = 1)
            or exists (select 1 from asset_network_card b where asset.id = b.asset_id and mac_address like
            CONCAT('%',replace(replace(#{multipleQuery},'%','\%'),'_','\_'),'%') and b.status = 1)
            or exists (select 1 from asset_network_equipment b where asset.id = b.asset_id and mac_address like
            CONCAT('%',replace(replace(#{multipleQuery},'%','\%'),'_','\_'),'%') and b.status = 1)
            or exists (select 1 from asset_safety_equipment b where asset.id = b.asset_id and mac like
            CONCAT('%',replace(replace(#{multipleQuery},'%','\%'),'_','\_'),'%') and b.status = 1)
            )
        </if>
        <if test=" installType != null">
            and (a.install_type = #{installType} or c.install_type = #{installType})
        </if>
        <if test=" assetId != null">
            and a.asset_id = #{assetId}c
        </if>
        group by
        a.id
    </select>
</mapper>
