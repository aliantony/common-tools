        and status = 1
    </select>

    <select id="findManufacturerName" resultType="string">
        select distinct(t3.manufacturer) from asset t1 inner join asset_software_relation t2 on t1.id=t2.asset_id
        inner join asset_software t3 on t3.id = t2.software_id
        <where>
            and t3.software_status != 5
            and t2.status =1
            and t1.status =1
            and t1.asset_status!=5
            <choose>
                <when test="manufacturerName != null and manufacturerName != ''">
                    and t3.manufacturer like CONCAT('%',#{manufacturerName},'%')
                </when>
                <otherwise>
                    and t3.manufacturer is not null
                </otherwise>
            </choose>

            <if test="areaIds != null and areaIds.size > 0">
                and t1.area_id in
                <foreach item="areaId" collection="areaIds" open="(" separator="," close=")">
                    #{areaId}
                </foreach>
            </if>
        </where>
    </select>

    <select id="countManufacturer" resultMap="ManufacturerResultMap" parameterType="java.util.List">
        select manufacturer, count(manufacturer) num from asset_software where 1=1
        <if test=" softwareStatusList != null and softwareStatusList.size() >0">
            and software_status in
            <foreach collection="softwareStatusList" open="(" close=")" separator="," index="index" item="item">
                #{item}
            </foreach>
        </if>
        and manufacturer is not null and status =1 GROUP BY manufacturer order by num desc
    </select>

    <select id="countStatus" resultMap="StatusResultMap">
        select software_status, count(software_status) num from asset_software  where status =1 GROUP BY software_status
    </select>

