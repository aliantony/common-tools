		<result column="important_degree" property="importantDegree" />
		<result column="name" property="name" />
		<result column="gmt_create" property="gmtCreate" />
		<result column="gmt_modified" property="gmtModified" />
		<result column="memo" property="memo" />
		<result column="create_user" property="createUser" />
		<result column="modify_user" property="modifyUser" />
		<result column="status" property="status" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, use, important_degree AS importantDegree, name, gmt_create AS gmtCreate, gmt_modified AS gmtModified, memo, create_user AS createUser, modify_user AS modifyUser, status
    </sql>

</mapper>
