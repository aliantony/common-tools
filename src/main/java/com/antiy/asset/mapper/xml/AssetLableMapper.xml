    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, label_type AS labelType, description, gmt_create AS gmtCreate, gmt_modified AS gmtModified, memo, create_user AS createUser, modify_user AS modifyUser, status
